{
  "hosting": {
    "public": "dist",
    "rewrites": [
      {
        "source": "/app/**",
        "function": "app"
      },
      {
        "source": "/v1/**",
        "function": "v1"
      },
      {
        "source": "/robots.txt",
        "destination": "/robots.txt"
      },
      {
        "source": "**",
        "function": "ssr"
      }
    ]
  }
}
